<?xml version="1.0" encoding="UTF-8"?>
<manifest>
	
	<!-- Hideki's GitHub -->
	<remote name="fh"
	fetch="https://github.com/fuyukihidekii/" />

	<!-- motorola-hanoip -->
	<!-- Device configuration -->
	<project path="device/motorola/hanoip" name="android_device_motorola_hanoip"
